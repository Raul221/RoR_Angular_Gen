{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "dependencies": {<% if (cssFramework === 'SASSBootstrap') { %>
    "sass-bootstrap": "*",<% } %><% if (includeFontAwesome) { %>
    "font-awesome": "*",<% } %><% if (includeUnderscore) { %>
    "underscore": "*",<% } %>
    "angular": "*",
    "angular-resource": "*",
    "angular-route": "*",<% if (includeUIBootstrap) { %>
    "angular-bootstrap": "*",<% } %>
    "jquery": "*",<% if (includeButtonCss) { %>
    "Buttons": "*",<% } %><% if (includeAnimateCss) { %>
    "animate-sass": "*",<% } %>
    "requirejs": "*",
    "modernizr": "*"
  }
}

